    {
      "name": "04. Auth API",
      "description": "Authentication API with JWT.\n\nRun: `cargo run --example auth_api`\nPort: 3000\n\nControllers:\n- AuthController: /api/auth (register, login)\n- UserController: /api/users (me)\n- AdminController: /api/admin (stats)",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"newuser@example.com\",\n  \"password\": \"SecurePassword123!\"\n}"
            },
            "url": {
              "raw": "{{base_url}}:3000/api/auth/register",
              "host": ["{{base_url}}"],
              "port": "3000",
              "path": ["api", "auth", "register"]
            },
            "description": "Register a new user account."
          }
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.token) {",
                  "    pm.environment.set('jwt_token', jsonData.token);",
                  "    console.log('JWT token saved to environment');",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"newuser@example.com\",\n  \"password\": \"SecurePassword123!\"\n}"
            },
            "url": {
              "raw": "{{base_url}}:3000/api/auth/login",
              "host": ["{{base_url}}"],
              "port": "3000",
              "path": ["api", "auth", "login"]
            },
            "description": "Login and receive a JWT token. Token is automatically saved to environment."
          }
        },
        {
          "name": "Get Current User (Protected)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}:3000/api/users/me",
              "host": ["{{base_url}}"],
              "port": "3000",
              "path": ["api", "users", "me"]
            },
            "description": "Get the current authenticated user. Requires JWT token."
          }
        },
        {
          "name": "Get Admin Stats (Admin Only)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}:3000/api/admin/stats",
              "host": ["{{base_url}}"],
              "port": "3000",
              "path": ["api", "admin", "stats"]
            },
            "description": "Get admin statistics. Requires admin role.\n\nUse admin credentials: admin@example.com / admin123"
          }
        }
      ]
    },
